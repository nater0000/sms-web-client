# .env.example
# Note: For this static site, enter these values in the "Settings" menu of the UI.
# Do not commit your actual secrets to Git!

# The public URL of your VPS Bridge (e.g. http://1.2.3.4:5000/webhook)
REACT_APP_BRIDGE_URL=

# The Webhook Secret defined in your /etc/default/voipms-sms-bridge
REACT_APP_BRIDGE_SECRET=
